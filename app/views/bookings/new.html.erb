<h1> <%= @tmp %> </h1>

<div class="booking-title">
  <h1>Réservez votre château</h1>
</div>

<div class="form-booking" style="width: 400px">
  <%= simple_form_for :search, method: "get" do |f| %>
    <%= f.input :Sélectionnez_vos_dates, as: :string, required: false, input_html: { id: "range_start" } %>
    <%#= f.button :submit, "Proposer mes dates", class: "btn btn-secondary" %>
  <% end %>


<%= simple_form_for([@castle, @booking]) do |f| %>
  <%= f.input :number_of_people %>
  <%= f.button :submit, "Réservez", class: "btn btn-secondary" %>
<% end %>
</div>
